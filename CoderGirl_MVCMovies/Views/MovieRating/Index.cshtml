
@{
    ViewData["Title"] = "Index";
}

<h1>Movie Ratings</h1>

<table>
    <tr>
        <th>Movie Name</th>
        <th>Movie Rating</th>
    </tr>
    @foreach (var movie in ViewBag.Movies)
    {
        <tr>
            <td>@movie.Value</td>
            <td>@movie.Rating</td>
        </tr>
    }
</table>